# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'N-Vibe' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for N-Vibe
pod 'SnapKit'

# Mapbox Maps SDK
pod 'MapboxMaps'

# Mapbox Search SDK
pod 'MapboxSearchUI'

# Mapbox Navigation SDK
pod 'MapboxNavigation', '~> 2.16'

# Mapbox Direction SDK
pod 'MapboxDirections'

# Mapbox Geocoder
pod 'MapboxGeocoder.swift'
end
